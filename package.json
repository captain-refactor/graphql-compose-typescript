{
  "name": "graphql-compose-typescript",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/janexpando/graphql-compose-typescript"
  },
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "npm run check && nyc ava",
    "test:watch": "ava --watch",
    "check": "tsc --noEmit",
    "build": "tsc",
    "coverage-report": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "keywords": [],
  "author": "Jan Kreme≈à",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/node": "^10.12.18",
    "ava": "^1.1.0",
    "codecov": "^3.1.0",
    "graphql": "^14.1.1",
    "graphql-compose": "^5.5.0",
    "injection-js": "^2.2.1",
    "nyc": "^13.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "graphql": "^14.1.1",
    "graphql-compose": "^5.5.0"
  },
  "ava": {
    "files": [
      "tests/**/*.ts",
      "!**/*.d.ts"
    ],
    "sources": [
      "src/**/*.ts"
    ],
    "require": [
      "reflect-metadata",
      "ts-node/register/transpile-only"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "dist",
      "tests",
      "examples",
      "**/*.d.ts"
    ],
    "all": true
  }
}
